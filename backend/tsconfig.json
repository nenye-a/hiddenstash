{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "lib": ["esnext"],
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "downlevelIteration": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true
  }
}
