{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start --web-only",
    "typecheck": "tsc --noEmit -p .",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx}\"",
    "lint": "eslint --max-warnings 0 \"src/**/*.{ts,tsx}\"",
    "jest": "jest --coverage --passWithNoTests",
    "test": "yarn lint && yarn typecheck && yarn format:check && yarn jest",
    "build": "expo build:web"
  },
  "dependencies": {
    "@stripe/react-stripe-js": "^1.1.2",
    "@stripe/stripe-js": "^1.4.0",
    "exoflex": "^3.3.1",
    "expo": "~37.0.3",
    "react": "~16.9.0",
    "react-dom": "~16.9.0",
    "react-is": "^16.13.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-37.0.1.tar.gz",
    "react-native-screens": "~2.2.0",
    "react-native-svg": "11.0.1",
    "react-native-vector-icons": "^6.6.0",
    "react-native-web": "~0.11.7",
    "react-router-dom": "^5.1.2",
    "styled-components": "^5.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.6",
    "@types/jest": "^25.2.1",
    "@types/react": "~16.9.23",
    "@types/react-native": "~0.61.17",
    "@types/react-router-dom": "^5.1.4",
    "@types/react-test-renderer": "^16.9.2",
    "@types/styled-components": "^5.1.0",
    "babel-preset-expo": "~8.1.0",
    "eslint": "^6.8.0",
    "eslint-config-kodefox": "^0.2.2",
    "expo-cli": "^3.18.6",
    "jest": "^25.3.0",
    "jest-expo": "^37.0.0",
    "prettier": "^2.0.4",
    "react-native-testing-library": "^1.13.0",
    "react-test-renderer": "^16.13.1",
    "ts-jest": "^25.3.1",
    "typescript": "^3.8.3"
  },
  "eslintConfig": {
    "extends": "kodefox/react-native"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "always"
  },
  "private": true
}
