{
  "manifest_version": 2,
  "name": "HiddenStash Chrome Extension",
  "description": "",
  "version": "0.0.1",
  "browser_action": {
    "default_icon": "./hiddenstash-icon.png"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["js/contentScript.js"]
    }
  ],
  "permissions": ["storage", "<all_urls>"],
  "web_accessible_resources": ["*.png", "*.svg"]
}
