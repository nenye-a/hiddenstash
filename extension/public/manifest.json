{
  "manifest_version": 2,
  "name": "HiddenStash Chrome Extension",
  "description": "",
  "version": "1.0",
  "browser_action": {
    "default_icon": "./hiddenstash-icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["js/contentScript.js"]
    }
  ],
  "permissions": ["storage", "<all_urls>"]
}
